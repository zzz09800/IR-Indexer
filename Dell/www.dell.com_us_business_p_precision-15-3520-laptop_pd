};
var chkBar = function() {
  if ($prcBar.css('position') == 'relative' && $prcBar.css('top') != '0')
    $prcBar.css('margin-top', '-' + ($facets.height() + 40) + 'px');
  else
    $prcBar.css('margin-top', '0');
  setTimeout(chkBar, 100);
};
